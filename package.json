{
  "name": "qrcode-transfer-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng s --host 0.0.0.0 --disable-host-check true -o",
    "build": "npm run ng-high-memory build --source-map && gzipper compress ./dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "ng-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng",
    "hmr": "ng s -o --hmr",
    "analyze": "npm run ng-high-memory build -- --source-map",
    "analyze:view": "source-map-explorer dist/**/*.js",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "prepare": "husky install",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "npx stylelint \"src/**/*.less\" --fix",
    "dev:ssr": "ng run qrcode-transfer-web:serve-ssr",
    "serve:ssr": "node dist/qrcode-transfer-web/server/main.js",
    "build:ssr": "ng build && ng run qrcode-transfer-web:server",
    "prerender": "ng run qrcode-transfer-web:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.1",
    "@delon/abc": "^18.1.0",
    "@delon/acl": "^18.1.0",
    "@delon/auth": "^18.1.0",
    "@delon/cache": "^18.1.0",
    "@delon/chart": "^18.1.0",
    "@delon/form": "^18.1.0",
    "@delon/mock": "^18.1.0",
    "@delon/theme": "^18.1.0",
    "@delon/util": "^18.1.0",
    "@ng-toolkit/universal": "8.1.0",
    "@nguniversal/express-engine": "^16.2.0",
    "@sec-ant/zxing-wasm": "^2.2.0",
    "@twilio/webrtc": "^4.6.0",
    "@types/md5": "^2.3.2",
    "@undecaf/zbar-wasm": "0.11.0",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.20.0",
    "detectrtc": "^1.4.1",
    "express": "^4.18.2",
    "filesize": "^10.1.4",
    "gzipper": "8.1.0",
    "md5": "^2.3.0",
    "ng-alain": "^18.1.0",
    "ng-zorro-antd": "^18.1.1",
    "ngx-webcam": "^0.4.1",
    "pako": "^2.1.0",
    "qrcode": "1.5.3",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "webrtc-adapter": "^9.0.1",
    "zbar-angular.wasm": "^1.0.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.1",
    "@angular-devkit/core": "18.2.1",
    "@angular-devkit/schematics": "18.2.1",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "^18.3.0",
    "@angular-eslint/eslint-plugin-template": "^18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "^18.3.0",
    "@angular/cli": "^18.2.1",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/language-service": "^18.2.1",
    "@delon/testing": "18.1.0",
    "@ng-util/monaco-editor": "^18.0.0",
    "@nguniversal/builders": "^16.2.0",
    "@schematics/angular": "18.2.1",
    "@types/dom-mediacapture-transform": "^0.1.6",
    "@types/dom-webcodecs": "^0.1.8",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@types/pako": "^2.0.0",
    "@types/qrcode": "^1.5.2",
    "@types/webrtc": "0.0.43",
    "@typescript-eslint/eslint-plugin": "~6.4.1",
    "@typescript-eslint/parser": "~6.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "3.0.0",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-jsdoc": "50.2.2",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^14.0.1",
    "ng-alain": "^18.1.0",
    "ng-alain-plugin-theme": "18.0.0",
    "ngx-tinymce": "^18.0.0",
    "prettier": "^3.0.2",
    "source-map-explorer": "^2.5.3",
    "stylelint": "16.8.2",
    "stylelint-config-clean-order": "^6.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "36.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-order": "^6.0.3",
    "typescript": "~5.5.2"
  },
  "lint-staged": {
    "(src)/**/*.{html,ts}": [
      "eslint --fix"
    ],
    "(src)/**/*.less": [
      "npm run lint:style"
    ]
  }
}
